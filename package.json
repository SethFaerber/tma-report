{
  "name": "strategic-maturity-analyzer",
  "version": "1.0.0",
  "description": "Strategic Maturity Assessment Analyzer - AI-powered team assessment reports",
  "main": "index.js",
  "engines": {
    "node": ">=24.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "install:all": "npm install && cd client && npm install && cd ../server && npm install",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "start": "cd server && npm start"
  },
  "keywords": [
    "strategic-maturity",
    "assessment",
    "claude-ai",
    "pdf-generation",
    "team-analysis"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
